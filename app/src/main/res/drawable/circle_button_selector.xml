<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- 버튼이 선택된 상태 -->
    <item android:state_selected="true" android:drawable="@drawable/circle_background_pressed" />

    <!-- 버튼을 클릭한 상태 -->
    <item android:state_pressed="true" android:drawable="@drawable/circle_background_pressed" />

    <!-- 기본 상태 -->
    <item android:drawable="@drawable/circle_background" />
</selector>
